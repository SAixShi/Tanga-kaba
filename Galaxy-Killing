TextButton.MouseButton1Click:Connect(function()
	if KeyTextbox.Text == SCRIPT_KEY then
		TextButton.Text = "Palo Correct! Loading..."
		task.wait(1)
		StartMainScript()
		ScreenGui:Destroy()
	else
		TextButton.Text = "Tang Ina Mo Mali! Try again."
		task.wait(2)
		TextButton.Text = "Submit Key"
	end
end)
 
